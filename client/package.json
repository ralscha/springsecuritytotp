{
  "name": "twofa",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "postbuild": "bread-compressor dist/app/browser",
    "format": "npx prettier --write ."
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.0.3",
    "@angular/common": "21.0.3",
    "@angular/compiler": "21.0.3",
    "@angular/core": "21.0.3",
    "@angular/forms": "21.0.3",
    "@angular/platform-browser": "21.0.3",
    "@angular/router": "21.0.3",
    "@ngx-loading-bar/core": "7.0.0",
    "@ngx-loading-bar/http-client": "7.0.0",
    "@primeuix/themes": "2.0.2",
    "angularx-qrcode": "21.0.4",
    "primeicons": "7.0.0",
    "primeng": "21.0.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "21.0.2",
    "@angular/cli": "21.0.2",
    "@angular/compiler-cli": "21.0.3",
    "angular-eslint": "21.0.1",
    "bread-compressor-cli": "3.1.2",
    "eslint": "9.39.1",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1"
  }
}
